// WAJP to create the method which will find largest of two numbers
class P4 
{
	public static void main(String[] args) 
	{
		large (300,50);
	}
	public static void large(int a , int b) 
	{
	    if (a>b)
	    {
			System.out.println(a+" is largest of the given two number");
	    }
		else 
		{
			System.out.println(b+" is largest of the given two number");
		}
	
	
	
	}	




}
