class Datatype 
{
	public static void main(String[] args) 
	{
		System.out.println("range of byte datatype is: ");
		System.out.println(Byte.MIN_VALUE);
		System.out.println(Byte.MIN_VALUE);
		System.out.println("***************");
		System.out.println("range of short datatype is:");
		System.out.println(Short.MIN_VALUE);
		System.out.println(Short.MIN_VALUE);
		System.out.println("***************");
		System.out.println("range of int datatype is:");
		System.out.println(Integer.MIN_VALUE);
		System.out.println(Integer.MIN_VALUE);
		System.out.println("***************");
		System.out.println("range of long datatype is:");
		System.out.println(Long.MIN_VALUE);
		System.out.println(Long.MIN_VALUE);
		System.out.println("***************"); 
		System.out.println("range of float datatype is:");
		System.out.println(Float.MIN_VALUE);
		System.out.println(Float.MIN_VALUE);
		System.out.println("***************");
		System.out.println("range of double datatype is:");
		System.out.println(Double.MIN_VALUE);
		System.out.println(Double.MAX_VALUE);
		System.out.println("***************");
	}
}
