//WAJP to find largest of three numbers create method to find largest number 
class  P2
{
	public static void main(String[] args) 
	{
		
		gone(10,20,30);
		
	}
	public static void gone(int a , int b , int c ) 
	{
		 int res = a<b ? b<c?c:b : a<c?c:a  ;
		System.out.println(res+" is largest");
	}
}
