//WAJP to find sum of the digits of the given number 
class  P6
{
	public static void main(String[] args) 
	{
		int a=123;
		int temp =a;
		int sum=0;
		while (a>0)
		{
			int rem=a%10;
			sum=rem + sum;
			a=a/10;
		}
		
		System.out.println("sum of digits of the given number"+temp+ "is  "+sum  );
	}
}
